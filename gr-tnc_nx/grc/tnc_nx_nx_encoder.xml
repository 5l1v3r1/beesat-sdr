<?xml version="1.0"?>
<block>
  <name>NX Encoder</name>
  <key>tnc_nx_nx_encoder</key>
  <category>TNC NX</category>
  <import>import tnc_nx</import>
  <make>tnc_nx.nx_encoder()</make>

  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>single</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>double</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  
</block>
